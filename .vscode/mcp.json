{
	"servers": {
		"workspace-mcp": {
			"command": "uvx",
			"args": [
				"workspace-mcp",
				"--single-user"
			],
			"envFile": "${workspaceFolder}/.env",
			"type": "stdio"
		},
		"atlassian-mcp": {
			"url": "https://mcp.atlassian.com/v1/sse",
			"type": "http"
		},
        "datadog-mcp": {
            "url": "https://mcp.datadoghq.com/api/unstable/mcp-server/mcp",
            "type": "http"
        }
	},
	"inputs": []
}